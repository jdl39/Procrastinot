{{> header}}

		<h1 class="text-center">Welcome to Procrastinot!</h1>
		<p>Welcome to Procrastinot! You can navigate through the app using the buttons at the bottom of the screen!</p>

		{{#each navBarButtons}}
		<div class="row">
			<div class="col-xs-3">
				<a href="{{url}}" class="btn btn-default homeButton"><img src="resources/{{imgFile}}" class="img-responsive" /></a>
			</div>
			<div class="col-xs-9">
				<h2 class="noTopMargin">{{name}}</h2>
				<p>{{description}}</p>
			</div>
		</div>
		{{/each}}
		

		</form>

		

{{> footer}}